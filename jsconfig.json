{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@modules/*": ["src/modules/*"],
      "@lib/*": ["src/lib/*"],
      "@router/*": ["src/router/*"],
      "@assets": ["src/assets/*"],
      "@hooks": ["src/hooks/*"],
      "@layout": ["src/layout/*"]
    }
  },
  "exclude": ["node_modules"]
}
